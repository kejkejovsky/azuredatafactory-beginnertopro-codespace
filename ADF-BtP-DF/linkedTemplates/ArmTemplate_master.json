{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-BtP-DF"
		},
		"az_ADLS_azfbtp_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'az_ADLS_azfbtp'"
		},
		"az_SQLDB_adf_btp_server_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'az_SQLDB_adf_btp_server'"
		},
		"az_ADLS_azfbtp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azfbtp.dfs.core.windows.net/"
		},
		"az_SQLDB_adf_btp_server_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adf-btp.database.windows.net"
		},
		"az_SQLDB_adf_btp_server_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adf-btp-db"
		},
		"az_SQLDB_adf_btp_server_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "CloudSA2d52d9ed"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-BtP-DF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"az_ADLS_azfbtp_accountKey": {
						"value": "[parameters('az_ADLS_azfbtp_accountKey')]"
					},
					"az_SQLDB_adf_btp_server_password": {
						"value": "[parameters('az_SQLDB_adf_btp_server_password')]"
					},
					"az_ADLS_azfbtp_properties_typeProperties_url": {
						"value": "[parameters('az_ADLS_azfbtp_properties_typeProperties_url')]"
					},
					"az_SQLDB_adf_btp_server_properties_typeProperties_server": {
						"value": "[parameters('az_SQLDB_adf_btp_server_properties_typeProperties_server')]"
					},
					"az_SQLDB_adf_btp_server_properties_typeProperties_database": {
						"value": "[parameters('az_SQLDB_adf_btp_server_properties_typeProperties_database')]"
					},
					"az_SQLDB_adf_btp_server_properties_typeProperties_userName": {
						"value": "[parameters('az_SQLDB_adf_btp_server_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}